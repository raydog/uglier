{
  "name": "uglier",
  "version": "0.4.1",
  "description": "A simple tool to take JS code and make it much, much worse",
  "keywords": [
    "ugly",
    "uglier",
    "format",
    "formatter",
    "code format",
    "prettier"
  ],
  "main": "src/index.js",
  "scripts": {
    "test": "ava",
    "coverage": "nyc -a -r html -r text-summary ava",
    "coveralls": "nyc -a -r text-lcov ava | coveralls"
  },
  "bin": {
    "uglier": "./src/main.js"
  },
  "author": "Ray Myers",
  "license": "MIT",
  "repository": "git@github.com:raydog/uglier.git",
  "dependencies": {
    "@babel/parser": "7.5.5",
    "chalk": "2.4.2",
    "globby": "10.0.1",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "ava": "2.2.0",
    "coveralls": "3.0.6",
    "nyc": "14.1.1",
    "temp": "0.9.0"
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/**/*.test.js"
    ]
  }
}
